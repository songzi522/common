        android:textSize="@dimen/dimen_20sp"
        />

    <Button
        android:id="@+id/btn2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_100dp"
        android:text="RxTest2Activity"
        android:textAllCaps="false"
        android:textSize="@dimen/dimen_20sp"
        />

    <Button
        android:id="@+id/btn3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_100dp"
        android:text=""
        android:textAllCaps="false"
        android:textSize="@dimen/dimen_20sp"
        />

</LinearLayout>
